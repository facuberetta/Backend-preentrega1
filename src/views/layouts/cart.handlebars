<h1>Tu Carrito</h1>

<table>
    <tr>
        <th>Producto</th>
        <th>Precio</th>
        <th>Cantidad</th>
    </tr>
    {{#each products}}
    <tr>
        <td>{{this.product.title}}</td>
        <td>${{this.product.price}}</td>
        <td>{{this.quantity}}</td>
    </tr>
    {{/each}}
</table>

<a href="/products">Seguir comprando</a>
